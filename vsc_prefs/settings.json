{
    "workbench.sideBar.location": "right",
    "workbench.statusBar.visible": false,
    "editor.fontFamily": "'SF Mono', 'Rec Mono Semicasual', 'monospace', 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "workbench.startupEditor": "none",
    "search.useIgnoreFiles": false,
    "editor.minimap.enabled": false,
    "editor.overviewRulerBorder": false,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "update.showReleaseNotes": false,
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "update.mode": "none",
    "editor.foldingHighlight": false,
    "editor.selectionHighlight": false,
    "editor.semanticHighlighting.enabled": false,
    "editor.occurrencesHighlight": false,
    "editor.matchBrackets": "never",
    "editor.renderLineHighlight": "all",
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#1a1b26",
        "editorCursor.foreground": "#4fb4d8",
        "terminalCursor.foreground": "#4fb4d8"
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "editor.guides.highlightActiveIndentation": false,
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.emptyWindow": false,
    "security.workspace.trust.startupPrompt": "never",
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.enablePersistentSessions": false,
    "files.hotExit": "off",
    "editor.lineNumbers": "relative",
    "notebook.lineNumbers": "on",
    "editor.guides.bracketPairs": true,
    /*"editor.renderWhitespace": "all",*/
    "code-eol.newlineCharacter": "↩",
    "code-eol.returnCharacter" : "¤",
    "code-eol.crlfCharacter": "¤¬",
    "workbench.editor.untitled.hint": "hidden",
    "telemetry.telemetryLevel": "off",
    "settingsSync.keybindingsPerPlatform": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Celestial",
    "python.defaultInterpreterPath": "/home/probe/anaconda3/bin/python",
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "editor.cursorStyle": "block",
    "notebook.output.fontSize": 14,
    "workbench.welcomePage.walkthroughs.openOnInstall": false,
    "editor.renderControlCharacters": false,
    "window.menuBarVisibility": "toggle",
    "workbench.layoutControl.enabled": false
}
